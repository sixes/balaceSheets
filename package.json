{
  "name": "banknote-app",
  "version": "1.0.0",
  "main": "dist/main.js",
  "description": "Standalone Excel-like accounting app",
  "scripts": {
    "dev": "concurrently \"vite --config vite.renderer.config.ts\" \"ts-node src/main/electron.ts\"",
    "build": "vite build --config vite.renderer.config.ts && tsc -p tsconfig.main.json",
    "package": "electron-builder"
  },
  "dependencies": {
    "ag-grid-community": "^31.0.0",
    "ag-grid-react": "^31.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "concurrently": "^8.2.0",
    "electron": "^29.0.0",
    "electron-builder": "^24.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  }
}